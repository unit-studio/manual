{
  "title": "unit",
  "description": "UnitStudio's technical manual repo.",
  "author": "unit-studio",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "root": ".",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md",
    "glossary": "GLOSSARY.md"
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 12,
    "fontFamily": "Arial",
    "paperSize": "a4",
    "margin": {
      "top": 56,
      "bottom": 56,
      "left": 62,
      "right": 62
    }
  },
  "links": {
    "sidebar": {

    }
  },
  "variables": {

  },
  "styles": {
    "website": "styles/website.css"
  },
  "plugins": [
    "-lunr",
    "-search",
    "-sharing",
    "advanced-emoji",
    "back-to-top-button",
    "code",
    "expandable-chapters-small",
    "favicon",
    "github",
    "hide-element",
    "popup",
    "search-pro",
    "sharing-plus",
    "splitter",
    "tbfed-pagefooter"
  ],
  "pluginsConfig": {
    "theme-default": {

    },
    "fontsettings": {
      "theme": "white",
      "family": "serif",
      "size": 1
    },
    "github": {
      "url": "https://github.com/unit-studio"
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy unit-studio 2019",
      "modify_label": "Last Edit:",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "favicon": {
      "shortcut": "assets/icons/favicon.png",
      "bookmark": "assets/icons/favicon.png",
      "appleTouch": "assets/icons/favicon.png",
      "appleTouchMore": {
        "120x120": "assets/icons/favicon.png",
        "180x180": "assets/icons/favicon.png"
      }
    }
  }
}
